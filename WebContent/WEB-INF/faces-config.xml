<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" version="2.0">
  <navigation-rule>
    <from-view-id>*</from-view-id>
    <navigation-case>
      <from-outcome>menuScreen</from-outcome>
      <to-view-id>/pages/menuScreen.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>emergency</from-outcome>
      <to-view-id>/pages/emergency.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>recipes</from-outcome>
      <to-view-id>/pages/testdatagrid.xhtml</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>shop</from-outcome>
      <to-view-id>/pages/shop.xhtml</to-view-id>
    </navigation-case>
  </navigation-rule>
  
  <managed-bean>
	 <managed-bean-name>userLoginControler</managed-bean-name>
	 <managed-bean-class>com.projet.controler.bean.UserLoginControler</managed-bean-class>
	 <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
</faces-config>
